{
  "name": "tinderclone-backend",
  "version": "1.0.0",
  "description": "Backend for TinderClone Devs",
  "main": "dist/App.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development nodemon --exec ts-node src/App.ts",
    "dev:staging": "NODE_ENV=staging nodemon --exec ts-node src/App.ts",
    "dev:prod": "NODE_ENV=production nodemon --exec ts-node src/App.ts",
    "start": "NODE_ENV=production node dist/App.js",
    "start:staging": "NODE_ENV=staging node dist/App.js",
    "start:dev": "NODE_ENV=development node dist/App.js",
    "clean": "rm -rf dist",
    "setup:local-db": "NODE_ENV=development node scripts/setupLocalDB.js",
    "sync:db:pull": "NODE_ENV=development node scripts/syncDatabase.js pull",
    "sync:db:push": "NODE_ENV=development node scripts/syncDatabase.js push",
    "db:reset": "npm run setup:local-db",
    "build:dev": "NODE_ENV=development npm run build",
    "build:staging": "NODE_ENV=staging npm run build",
    "build:prod": "NODE_ENV=production npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CS-VineetKumar/tinderClone-backend.git"
  },
  "keywords": [
    "NodeJs",
    "TypeScript"
  ],
  "author": "Vineet Kumar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CS-VineetKumar/tinderClone-backend/issues"
  },
  "homepage": "https://github.com/CS-VineetKumar/tinderClone-backend#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "nodemon": "^3.1.10",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.11.0",
    "@types/validator": "^13.11.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}